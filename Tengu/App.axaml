<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Tengu"
			 xmlns:sty="using:FluentAvalonia.Styling"
             x:Class="Tengu.App">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source='/Resources/Icons.axaml'/>
				<ResourceInclude Source='/Resources/Itachi.axaml'/>
				<ResourceInclude Source='/Resources/Killua.axaml'/>
				<ResourceInclude Source='/Resources/Yato.axaml'/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>

    <Application.Styles>
		
		<sty:FluentAvaloniaTheme />
		<StyleInclude Source="avares://Material.Icons.Avalonia/App.xaml" />

    </Application.Styles>
</Application>
